<AnchorPane prefHeight="1250.0" prefWidth="1400.0" styleClass="zishan" stylesheets="@options.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo4.ZishanJavaClass.IntercroppingController">
   <children>
      <Label layoutX="474.0" layoutY="66.0" prefHeight="46.0" prefWidth="404.0" styleClass="head-label" text="                   Intercropping" textFill="WHITE">
         <font>
            <Font size="24.0" />
         </font></Label>
      <ComboBox fx:id="cropComboBox" layoutX="550.0" layoutY="166.0" prefHeight="26.0" prefWidth="254.0" promptText="                Select Crop to Intercrop" styleClass="gradient">
         <items>
            <FXCollections fx:factory="observableArrayList" />
         </items>
      </ComboBox>
      <Button layoutX="650.0" layoutY="230.0" onAction="#ButtonOk" prefHeight="26.0" prefWidth="58.0" styleClass="create-btn" stylesheets="@options.css" text="OK" />
      <ScrollPane layoutX="184.0" layoutY="353.0" prefHeight="353.0" prefWidth="1032.0" stylesheets="@options.css">
         <content>
            <Label fx:id="intercroppingProcedureLabel" prefHeight="894.0" prefWidth="1014.0" styleClass="gradient" stylesheets="@options.css" wrapText="true" />
         </content></ScrollPane>
      <Button alignment="CENTER" layoutX="182.0" layoutY="298.0" onAction="#OnVideoLinkclk" prefHeight="30.0" prefWidth="267.0" styleClass="find-btn" text="View Intercropping Video" />
      <Button layoutX="36.0" layoutY="76.0" mnemonicParsing="false" onAction="#onBtnClk" prefHeight="26.0" prefWidth="98.0" styleClass="create-btn" stylesheets="@options.css" text="BACK" />
   </children>
